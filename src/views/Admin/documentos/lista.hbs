<div class= "card">
    <div class="text-center">
        <h3 class="card-header class-header-primary"></h3>
    </div>
    <table class="table">
        <thead class="text-center">
        <tr>
            <th>Nombre </th>
            <th>Tipo </th>
            <th class="text-right">Acciones</th>
        </tr>
        </thead>
        {{#each documentos}}
            <tbody>
            <tr class="text-center">
                <td>{{Nombre}}</td>
                <td>{{Tipo}}</td>
                <td class="td-actions text-center ">
                    <div class="nav-tabs-wrapper">
                        <ul class="nav flex-column ">
                            <a class="nav-link" data-toggle="collapse" href="#{{_id}}">
                                <p> Acciones </p>
                            </a>
                            <div class="collapse" id={{_id}}>
                                <li class="nav-item">
                                    <a href="/clientes/documento/{{_id}}">Subir documento</a>
                                </li>
                                <li class="nav-item">
                                    <a href="/clientes/documento/{{_id}}">Documentos</a>
                                </li>
                            </div>
                        </ul>
                    </div>
                </td>
                <td>
                    <button type="button" rel="tooltip" class="btn btn-danger">
                        <a class="material-icons" href="/clientes/delete/{{_id}}">
                            delete
                        </a>
                    </button>
                </td>
            </tr>
            </tbody>
        {{/each}}
    </table>
</div>